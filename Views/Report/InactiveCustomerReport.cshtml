@model IEnumerable<GroupCourseWork.ViewModels.InactiveCustomerViewModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>


<table class="table table-bordered">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CustomerName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastBoughtDate.Date)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.CustomerName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastBoughtDate.Date)
                </td>
            </tr>
        }
    </tbody>
</table>

@model IEnumerable<GroupCourseWork.ViewModels.InactiveProductViewModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>


<table class="table table-bordered">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ProductName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastBoughtDate)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ProductName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastBoughtDate)
            </td>

        </tr>
        }
    </tbody>
</table>
